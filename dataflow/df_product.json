{
	"name": "df_product",
	"properties": {
		"folder": {
			"name": "df.parquet"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_raw_product",
						"type": "DatasetReference"
					},
					"name": "sourceproduct"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "tabelaProduct",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductID as string,",
				"          Name as string,",
				"          ProductNumber as string,",
				"          MakeFlag as string,",
				"          FinishedGoodsFlag as string,",
				"          Color as string,",
				"          SafetyStockLevel as string,",
				"          ReorderPoint as string,",
				"          StandardCost as string,",
				"          ListPrice as string,",
				"          Size as string,",
				"          SizeUnitMeasureCode as string,",
				"          WeightUnitMeasureCode as string,",
				"          Weight as string,",
				"          DaysToManufacture as string,",
				"          ProductLine as string,",
				"          Class as string,",
				"          Style as string,",
				"          ProductSubcategoryID as string,",
				"          ProductModelID as string,",
				"          SellStartDate as string,",
				"          SellEndDate as string,",
				"          DiscontinuedDate as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceproduct",
				"sourceproduct select(mapColumn(",
				"          ProductID,",
				"          Name,",
				"          ProductNumber,",
				"          MakeFlag,",
				"          FinishedGoodsFlag,",
				"          Color,",
				"          SafetyStockLevel,",
				"          ReorderPoint,",
				"          StandardCost,",
				"          ListPrice,",
				"          Size,",
				"          SizeUnitMeasureCode,",
				"          WeightUnitMeasureCode,",
				"          Weight,",
				"          DaysToManufacture,",
				"          ProductLine,",
				"          Class,",
				"          Style,",
				"          ProductSubcategoryID,",
				"          ProductModelID,",
				"          SellStartDate,",
				"          SellEndDate,",
				"          rowguid,",
				"          ModifiedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 cast(output(",
				"          ProductID as integer '000',",
				"          MakeFlag as integer '000',",
				"          FinishedGoodsFlag as integer '000',",
				"          SafetyStockLevel as integer '000',",
				"          ReorderPoint as integer '000',",
				"          StandardCost as double '000,000,000.000',",
				"          ListPrice as double '000,000,000.000',",
				"          DaysToManufacture as integer '000'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 derive(NewSellStartDate = toTimestamp(SellStartDate, 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          NewModifiedDate = toTimestamp(ModifiedDate, 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          NewSellEndDate = toTimestamp(SellEndDate, 'yyyy-MM-dd HH:mm:ss.SSS')) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          ProductID,",
				"          Name,",
				"          ProductNumber,",
				"          MakeFlag,",
				"          FinishedGoodsFlag,",
				"          Color,",
				"          SafetyStockLevel,",
				"          ReorderPoint,",
				"          StandardCost,",
				"          ListPrice,",
				"          Size,",
				"          SizeUnitMeasureCode,",
				"          WeightUnitMeasureCode,",
				"          Weight,",
				"          DaysToManufacture,",
				"          ProductLine,",
				"          Class,",
				"          Style,",
				"          ProductSubcategoryID,",
				"          ProductModelID,",
				"          rowguid,",
				"          NewSellStartDate,",
				"          NewModifiedDate,",
				"          NewSellEndDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          ProductID as integer,",
				"          Name as string,",
				"          ProductNumber as string,",
				"          MakeFlag as integer,",
				"          FinishedGoodsFlag as integer,",
				"          Color as string,",
				"          SafetyStockLevel as integer,",
				"          ReorderPoint as integer,",
				"          StandardCost as decimal(18,2),",
				"          ListPrice as decimal(18,2),",
				"          Size as string,",
				"          SizeUnitMeasureCode as string,",
				"          WeightUnitMeasureCode as string,",
				"          Weight as string,",
				"          DaysToManufacture as integer,",
				"          ProductLine as string,",
				"          Class as string,",
				"          Style as string,",
				"          ProductSubcategoryID as string,",
				"          ProductModelID as string,",
				"          rowguid as string,",
				"          SellStartDate as timestamp,",
				"          ModifiedDate as timestamp,",
				"          SellEndDate as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1"
			]
		}
	}
}
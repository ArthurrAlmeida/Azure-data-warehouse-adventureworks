{
	"name": "df_salesOrderHeader",
	"properties": {
		"folder": {
			"name": "df.parquet"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_raw_salesOrderHeader",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "tabelaSalesOrderHeader",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "cast1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SalesOrderID as string,",
				"          RevisionNumber as string,",
				"          OrderDate as string,",
				"          DueDate as string,",
				"          ShipDate as string,",
				"          Status as string,",
				"          OnlineOrderFlag as string,",
				"          SalesOrderNumber as string,",
				"          PurchaseOrderNumber as string,",
				"          AccountNumber as string,",
				"          CustomerID as string,",
				"          SalesPersonID as string,",
				"          TerritoryID as string,",
				"          BillToAddressID as string,",
				"          ShipToAddressID as string,",
				"          ShipMethodID as string,",
				"          CreditCardID as string,",
				"          CreditCardApprovalCode as string,",
				"          CurrencyRateID as string,",
				"          SubTotal as string,",
				"          TaxAmt as string,",
				"          Freight as string,",
				"          TotalDue as string,",
				"          Comment as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     limit: 100,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 select(mapColumn(",
				"          SalesOrderID,",
				"          RevisionNumber,",
				"          OrderDate,",
				"          DueDate,",
				"          ShipDate,",
				"          Status,",
				"          OnlineOrderFlag,",
				"          SalesOrderNumber,",
				"          PurchaseOrderNumber,",
				"          AccountNumber,",
				"          CustomerID,",
				"          SalesPersonID,",
				"          TerritoryID,",
				"          BillToAddressID,",
				"          ShipToAddressID,",
				"          ShipMethodID,",
				"          CreditCardID,",
				"          CreditCardApprovalCode,",
				"          CurrencyRateID,",
				"          SubTotal,",
				"          TaxAmt,",
				"          Freight,",
				"          TotalDue,",
				"          Comment,",
				"          rowguid,",
				"          ModifiedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 cast(output(",
				"          SalesOrderID as integer '000',",
				"          RevisionNumber as integer '000',",
				"          Status as integer '000',",
				"          OnlineOrderFlag as integer '000',",
				"          CustomerID as integer '000',",
				"          TerritoryID as integer '000',",
				"          BillToAddressID as integer '000',",
				"          ShipToAddressID as integer '000',",
				"          CreditCardID as integer '000',",
				"          SubTotal as float '000.##',",
				"          TaxAmt as float '000.##',",
				"          Freight as float '000.##',",
				"          TotalDue as float '000.##'",
				"     ),",
				"     errors: true) ~> cast1",
				"cast1 derive(OrderDate = toTimestamp(OrderDate, 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          DueDate = toTimestamp(DueDate, 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          ShipDate = toTimestamp(ShipDate, 'yyyy-MM-dd HH:mm:ss.SSS'),",
				"          ModifiedDate = toTimestamp(ModifiedDate, 'yyyy-MM-dd HH:mm:ss.SSS')) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          SalesOrderID,",
				"          RevisionNumber,",
				"          OrderDate,",
				"          DueDate,",
				"          ShipDate,",
				"          Status,",
				"          OnlineOrderFlag,",
				"          SalesOrderNumber = SalesPersonID,",
				"          PurchaseOrderNumber,",
				"          AccountNumber,",
				"          CustomerID,",
				"          SalesPersonID,",
				"          TerritoryID,",
				"          BillToAddressID,",
				"          ShipToAddressID,",
				"          ShipMethodID,",
				"          CreditCardID,",
				"          CreditCardApprovalCode,",
				"          CurrencyRateID,",
				"          SubTotal,",
				"          TaxAmt,",
				"          Freight,",
				"          TotalDue,",
				"          rowguid,",
				"          ModifiedDate",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SalesOrderID as integer,",
				"          RevisionNumber as integer,",
				"          OrderDate as timestamp,",
				"          DueDate as timestamp,",
				"          ShipDate as timestamp,",
				"          Status as integer,",
				"          OnlineOrderFlag as integer,",
				"          SalesOrderNumber as string,",
				"          PurchaseOrderNumber as string,",
				"          AccountNumber as string,",
				"          CustomerID as integer,",
				"          TerritoryID as integer,",
				"          BillToAddressID as integer,",
				"          ShipToAddressID as integer,",
				"          ShipMethodID as string,",
				"          CreditCardID as integer,",
				"          CreditCardApprovalCode as string,",
				"          CurrencyRateID as string,",
				"          SubTotal as decimal(18,2),",
				"          TaxAmt as decimal(18,2),",
				"          Freight as decimal(18,2),",
				"          TotalDue as decimal(18,2),",
				"          rowguid as string,",
				"          ModifiedDate as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     preCommands: [],",
				"     postCommands: []) ~> sink1"
			]
		}
	}
}